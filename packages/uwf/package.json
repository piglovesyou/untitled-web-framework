{
  "name": "uwf",
  "version": "0.0.0",
  "engines": {
    "node": ">=10.15.3"
  },
  "bin": {
    "uwf": "bin/uwf"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "main": "src/exports.ts",
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "@piglovesyou/isomorphic-style-loader": "^4.1.1",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "apollo-link-logger": "^1.2.3",
    "apollo-link-schema": "^1.2.2",
    "apollo-server": "^2.6.2",
    "apollo-server-express": "^2.4.8",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "history": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.merge": "^4.6.1",
    "node-fetch": "^2.3.0",
    "passport": "^0.4.0",
    "path-to-regexp": "3.0.0",
    "pretty-error": "^2.1.1",
    "query-string": "^6.4.0",
    "react-apollo": "^2.5.2",
    "react-apollo-hooks": "^0.4.5",
    "serialize-javascript": "^1.6.1",
    "source-map-support": "^0.5.11",
    "sqlite3": "^4.0.8",
    "universal-router": "^8.2.0",
    "whatwg-fetch": "^3.0.0"
  },
  "lint-staged": {
    "ignore": [
      "package.json"
    ],
    "linters": {
      "*.{js,jsx}": [
        "npm run fix-js",
        "git add --force"
      ],
      "*.{json,md,graphql}": [
        "prettier --write",
        "git add --force"
      ],
      "*.{css,less,styl,scss,sass,sss}": [
        "stylelint --fix",
        "git add --force"
      ]
    }
  },
  "scripts": {
    "test": "jest",
    "test-watch": "yarn run test --watch --notify",
    "test-cover": "yarn run test --coverage",
    "coverage": "yarn run test-cover && opn coverage/lcov-report/index.html",
    "build-stats": "yarn run build --release --analyse",
    "watch": "babel --watch --source-maps inline -d src/tools -x '.ts' tools",
    "prepack": "rimraf dist && copyfiles -u 1 src/**/*.css dist && babel --extensions '.ts,.tsx,.js' -d dist src"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.3.0",
    "@graphql-codegen/typescript": "1.2.0",
    "@graphql-codegen/typescript-operations": "^1.2.0",
    "@graphql-codegen/typescript-react-apollo": "^1.2.0",
    "@piglovesyou/enhanced-resolve": "^4.3.0",
    "copyfiles": "^2.1.0",
    "typesync": "^0.5.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}
